CREATE MEMORY TABLE AF_CHANNELS(NAME VARCHAR(256) NOT NULL,PCLASSNAME VARCHAR(256),ECLASSNAME VARCHAR(256),DESCRIPTOR LONGVARCHAR,LCLASSNAME VARCHAR(256),TARGET VARCHAR(256),DESCRIPTION VARCHAR(256),CONSTRAINT PK_AF_CHANNELS PRIMARY KEY(NAME))
CREATE MEMORY TABLE IBS_SOAP_SERVERS(SOAP_SERVER_NAME VARCHAR(256) NOT NULL,SERVER_HOST VARCHAR(256),SERVER_PORT VARCHAR(256),SERVER_DEFAULT_KEY INTEGER,CONSTRAINT PK_IBS_SOAP_SERVERS PRIMARY KEY(SOAP_SERVER_NAME))
