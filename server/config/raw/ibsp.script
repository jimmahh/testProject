CREATE MEMORY TABLE IBS_LICENSES(SOAP_SERVER_NAME VARCHAR(256) NOT NULL,LICENSE_NAME VARCHAR(256) NOT NULL,LICENSE_DESC VARCHAR(256),MODIFY_TIME TIMESTAMP,CREATE_TIME TIMESTAMP,CONSTRAINT PK_IBS_LICENSES PRIMARY KEY(SOAP_SERVER_NAME,LICENSE_NAME))
