CREATE MEMORY TABLE IBS_METHODS(SERVICE_NAME VARCHAR(256) NOT NULL,METHOD_NAME VARCHAR(256) NOT NULL,METHOD_TYPE INTEGER,METHOD_ARGUMENTS LONGVARCHAR,METHOD_CREATE_TIME TIMESTAMP,METHOD_RPC_PWD VARCHAR(256),METHOD_DESCRIPTION VARCHAR(256),METHOD_PUB_TIME TIMESTAMP,METHOD_MOD_TIME TIMESTAMP,METHOD_RESULTS LONGVARCHAR,METHOD_RPC_SERVER VARCHAR(256),IBO_AGENT_NAME LONGVARCHAR,METHOD_RPC_NAME VARCHAR(256),METHOD_RPC_USER LONGVARCHAR,CONSTRAINT PK_IBS_METHODS PRIMARY KEY(SERVICE_NAME,METHOD_NAME))
CREATE MEMORY TABLE IBS_POLICY_TYPE(POLICY_TYPE_NAME VARCHAR(256) NOT NULL,POLICY_TYPE_DESCRIPTION VARCHAR(256),POLICY_TYPE_ID INTEGER,CONSTRAINT PK_IBS_POLICY_TYPE PRIMARY KEY(POLICY_TYPE_NAME))
CREATE MEMORY TABLE IBS_STATES(STATEID INTEGER NOT NULL,NAME VARCHAR(256),DESCRIPTION VARCHAR(256),CONSTRAINT PK_IBS_STATES PRIMARY KEY(STATEID))
