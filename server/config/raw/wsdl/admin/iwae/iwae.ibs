							<xs:attributeGroup ref="tns:nkdm"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="method">
						<xs:annotation>
							<xs:documentation>web method</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
