										<xs:enumeration value="ADPRPC"/>
										<xs:enumeration value="ADPRPC2"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element name="svclicense">
						<xs:annotation>
							<xs:documentation>simplified license</xs:documentation>
