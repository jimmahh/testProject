						<xs:complexType>
							<xs:sequence>
								<xs:element name="service_name" type="xs:string">
									<xs:annotation>
										<xs:documentation>service name retrieved from GET_TARGET_FROM_WS_METHOD</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="method_name" type="xs:string">
									<xs:annotation>
										<xs:documentation>method name retrieved from GET_TARGET_FROM_WS_METHOD</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:schema>
			</input>
			<output>
				<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="urn:schemas-iwaysoftware-com:dec2002:iwse:af">
					<xs:element name="GET_TARGET_FROM_WS_METHODResponse">
						<xs:annotation>
							<xs:documentation>Use this method to get what target is associated with a given WS Method</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="tns:target" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="target">
						<xs:annotation>
							<xs:documentation>Container for target information</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="action" type="tns:actionType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>enumeration with values update, new, delete. update is used by default</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="tid" type="xs:string" minOccurs="0">
									<xs:annotation>
										<xs:documentation>target id generated during installation/configuration</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="target" type="xs:string">
									<xs:annotation>
										<xs:documentation>adapter target name retrieved from GETADAPTERINFO</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="name" type="xs:string">
									<xs:annotation>
										<xs:documentation>administrator assigned name of the target</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="description" type="xs:string">
									<xs:annotation>
										<xs:documentation>administrator assigned description of the target</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element ref="tns:descriptor"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:simpleType name="actionType">
						<xs:annotation>
							<xs:documentation>defines enumeration of possible actions</xs:documentation>
						</xs:annotation>
						<xs:restriction base="xs:string">
							<xs:enumeration value="update"/>
							<xs:enumeration value="new"/>
							<xs:enumeration value="delete"/>
						</xs:restriction>
					</xs:simpleType>
					<xs:element name="descriptor">
						<xs:annotation>
							<xs:documentation>The descriptor describes all the possible configuration option(s) for an adapter. It can also be used to  describe an instance of a particular configuration.</xs:documentation>
						</xs:annotation>
